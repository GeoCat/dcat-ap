<?xml version="1.0" encoding="UTF-8"?>
<editor xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:gn="http://www.fao.org/geonetwork"
        xmlns:spdx="http://spdx.org/rdf/terms#"
        xmlns:skos="http://www.w3.org/2004/02/skos/core#"
        xmlns:adms="http://www.w3.org/ns/adms#"
        xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:dc="http://purl.org/dc/elements/1.1/"
        xmlns:dct="http://purl.org/dc/terms/"
        xmlns:dcat="http://www.w3.org/ns/dcat#"
        xmlns:vcard="http://www.w3.org/2006/vcard/ns#"
        xmlns:foaf="http://xmlns.com/foaf/0.1/"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:mdcat="https://data.vlaanderen.be/ns/metadata-dcat#"
        xmlns:schema="http://schema.org/"
        xmlns:locn="http://www.w3.org/ns/locn#"
        xsi:noNamespaceSchemaLocation="../../../../../../config-editor.xsd">
  <!-- Define form field types per metadata element. -->
  <fields/>
  <!-- Complex fields. -->
  <fieldsWithFieldset/>

  <multilingualFields>
    <exclude>
      <name>dct:identifier</name>
    </exclude>
  </multilingualFields>

  <views>
    <view name="default" displayTooltips="true" upAndDownControlHidden="true">
      <sidePanel>
        <directive data-gn-overview-manager=""/>
        <directive data-gn-onlinesrc-list="" data-types="service|dataset"/>
        <directive data-gn-validation-report=""/>
      </sidePanel>

      <tab id="defaultDataset" default="true" mode="flat" displayIfRecord="count(//dcat:dataset) > 0" hideIfNotDisplayed="true">
        <text><h3>defaultDataset tab - TODO</h3></text>
      </tab>
      <tab id="defaultDistribution" mode="flat" displayIfRecord="count(//dcat:dataset) > 0" hideIfNotDisplayed="true">
        <text><h3>defaultDistribution tab - TODO</h3></text>
      </tab>
      <tab id="defaultService" default="true" mode="flat" displayIfRecord="count(//dcat:service) > 0" hideIfNotDisplayed="true">
        <text><h3>defaultService tab - TODO</h3></text>
      </tab>
      <tab id="defaultRecord" mode="flat">
        <text><h3>defaultRecord tab - TODO</h3></text>
      </tab>
      <flatModeExceptions/>
      <thesaurusList/>
    </view>

    <view name="advanced" displayTooltips="true" upAndDownControlHidden="true">
      <sidePanel>
        <directive data-gn-overview-manager=""/>
        <directive data-gn-onlinesrc-list="" data-types="service|dataset"/>
        <directive data-gn-validation-report=""/>
      </sidePanel>
      <tab id="advancedDataset" default="true" displayIfRecord="count(//dcat:dataset) > 0" hideIfNotDisplayed="true"></tab>
      <tab id="advancedDistribution" displayIfRecord="count(//dcat:dataset) > 0" hideIfNotDisplayed="true"></tab>
      <tab id="advancedService" default="true" displayIfRecord="count(//dcat:service) > 0" hideIfNotDisplayed="true"></tab>
      <tab id="advancedRecord" mode="flat"></tab>
      <flatModeExceptions></flatModeExceptions>
      <thesaurusList></thesaurusList>
    </view>

    <view name="xml">
      <sidePanel>
        <directive data-gn-validation-report=""/>
      </sidePanel>
      <tab id="xml" default="true"/>
    </view>
  </views>
  <batchEditing>
    <section name="identification">
      <field name="title" xpath="/rdf:RDF/dcat:Catalog/dcat:dataset/dcat:Dataset/dct:title"/>
    </section>
  </batchEditing>
</editor>
