        xsi:noNamespaceSchemaLocation="../../../../../../config-editor.xsd">
  <!-- Define form field types per metadata element. -->
  <fields>
    <for name="dct:description" use="textarea"/>
    <for name="dct:type" xpath="foaf:Agent" use="thesaurus-list-picker">
      <directiveAttributes
        thesaurus="external.theme.publisher-type"
        xpath="/foaf:Agent/dct:type"
        max="1"
        labelKey="dcat.addType"/>
    </for>
    <for name="dcat:theme" use="thesaurus-list-picker">
      <directiveAttributes
        thesaurus="external.theme.datatheme"
        xpath="/dcat:theme"
        max=""
        labelKey="dcat.addThemes"/>
    </for>
    <for name="mdcat:MAGDA-categorie" use="thesaurus-list-picker" profile="metadata-dcat">
      <directiveAttributes
        thesaurus="external.theme.magda-domain"
        xpath="/mdcat:MAGDA-categorie"
        max=""
        labelKey="mdcat.addMagdaCategorie"/>
    </for>
    <for name="mdcat:statuut" use="thesaurus-list-picker">
      <directiveAttributes
        thesaurus="external.theme.GDI-Vlaanderen-trefwoorden"
        xpath="/mdcat:statuut"
        max=""
        labelKey="mdcat.addStatuut"/>
    </for>
    <for name="dct:accessRights" xpath="dcat:Dataset" use="thesaurus-list-picker">
      <directiveAttributes
        thesaurus="external.theme.access-right"
        xpath="/dct:accessRights"
        max="1"
        labelKey="dcat.addAccessRight"/>
    </for>
    <for name="dct:accessRights" xpath="dcat:DataService" use="thesaurus-list-picker">
      <directiveAttributes
        thesaurus="external.theme.access-right-service"
        xpath="/dct:accessRights"
        max="1"
        labelKey="dcat.addAccessRight"/>
    </for>
    <for name="dct:accessRights" xpath="dcat:Distribution" use="thesaurus-list-picker">
      <directiveAttributes
        thesaurus="external.theme.access-right"
        xpath="/dcat:Distribution/dct:accessRights"
        max="1"
        labelKey="dcat.addAccessRight"/>
    </for>
    <for name="dct:language" xpath="dcat:Dataset" use="thesaurus-list-picker">
      <directiveAttributes
        thesaurus="external.theme.language"
        xpath="/dct:language"
        max=""
        labelKey="dcat.addLanguage"/>
    </for>
    <for name="dct:language" xpath="dcat:DataService" use="thesaurus-list-picker">
      <directiveAttributes
        thesaurus="external.theme.language"
        xpath="/dct:language"
        max=""
        labelKey="dcat.addLanguage"/>
    </for>
    <for name="dct:language" xpath="dcat:Distribution" use="thesaurus-list-picker">
      <directiveAttributes
        thesaurus="external.theme.language"
        xpath="/dcat:Distribution/dct:language"
        max=""
        labelKey="dcat.addLanguage"/>
    </for>
    <for name="dct:conformsTo"
         addDirective="data-gn-directory-entry-selector"
         xpath="/rdf:RDF/dcat:Catalog/dcat:service/dcat:DataService/dct:conformsTo">
      <directiveAttributes
        data-template-add-action="false"
        data-search-action="true"
        data-popup-action="false"
        data-template-type="protocol"
        data-filter='{"_root": "dct:Standard"}'
        data-insert-modes="text"/>
    </for>

    <for name="dct:created" templateModeOnly="true" forceLabel="true" label="key">
      <template>
        <values>
          <key label="key" xpath="." use="gn-date-picker">
            <directiveAttributes data-tag-name="dct:created"/>
          </key>
        </values>
        <snippet>
          <dct:created>{{key}}</dct:created>
        </snippet>
      </template>
    </for>
    <for name="dct:issued" templateModeOnly="true" forceLabel="true" label="key">
      <template>
        <values>
          <key label="key" xpath="." use="gn-date-picker">
            <directiveAttributes data-tag-name="dct:issued"/>
          </key>
        </values>
        <snippet>
          <dct:issued>{{key}}</dct:issued>
        </snippet>
      </template>
    </for>
    <for name="dct:modified" templateModeOnly="true" forceLabel="true" label="key">
      <template>
        <values>
          <key label="key" xpath="." use="gn-date-picker">
            <directiveAttributes data-tag-name="dct:modified"/>
          </key>
        </values>
        <snippet>
          <dct:modified>{{key}}</dct:modified>
        </snippet>
      </template>
    </for>
    <for name="mdcat:levensfase" use="thesaurus-list-picker">
      <directiveAttributes
        thesaurus="external.theme.levensfase"
        xpath="/mdcat:levensfase"
        max="1"
        labelKey="mdcat.addLevensfase"/>
    </for>
    <for name="mdcat:ontwikkelingstoestand" use="thesaurus-list-picker">
      <directiveAttributes
        thesaurus="external.theme.ontwikkelingstoestand"
        xpath="/mdcat:ontwikkelingstoestand"
        max="1"
        labelKey="mdcat.addOntwikkelingstoestand"/>
    </for>
    <for name="dct:conformsTo" addDirective="data-gn-directory-entry-selector"
         xpath="/rdf:RDF/dcat:Catalog/dcat:service/dcat:DataService/dct:conformsTo">
      <directiveAttributes
        data-template-add-action="false"
        data-search-action="true"
        data-popup-action="false"
        data-template-type="protocol"
        data-filter='{"_root": "dct:Standard"}'
        data-insert-modes="text"/>
    </for>

  </fields>
  <!-- Complex fields. -->
  <fieldsWithFieldset>
    <name isSupportingSlideToggle="true">dcat:contactPoint</name>
    <name isSupportingSlideToggle="true">dct:publisher</name>
    <name isSupportingSlideToggle="true">dct:accessRights</name>
