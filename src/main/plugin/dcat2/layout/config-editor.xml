    <name isSupportingSlideToggle="true">dct:conformsTo</name>
    <name>dcat:qualifiedRelation</name>
    <name>dct:rights</name>
    <name>dct:creator</name>
    <name>dct:conformsTo</name>
  </fieldsWithFieldset>

